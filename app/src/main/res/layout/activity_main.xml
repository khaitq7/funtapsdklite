<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_call_form_sdk"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Call to form login"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/txt_token"
        android:text="token :"
        app:layout_constraintTop_toBottomOf="@id/btn_call_form_sdk"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/txt_ftid"
        android:text="ft_id :"
        app:layout_constraintTop_toBottomOf="@id/txt_token"/>
</androidx.constraintlayout.widget.ConstraintLayout>